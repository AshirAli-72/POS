  <DataSets>
    <DataSet Name="persondataset">
      <Query>
        <DataSourceName>installment_dbDataSet1</DataSourceName>
        <CommandText>
					SELECT
					p.id,
					p.name,
					p.mobile_number,
					p.cnic_number,
					p.address,
					p.status,
					COALESCE(cm.amount, 0) AS amount,
					ISNULL(pt.payment_title, 'Cash') AS payment_type
					FROM dbo.pos_persons AS p
					LEFT JOIN dbo.pos_cash_management AS cm
					ON cm.person_id = p.id
					LEFT JOIN dbo.pos_payment_type AS pt
					ON cm.payment_id = pt.id
				</CommandText>
      </Query>
      <Fields>
        <Field Name="name">
          <DataField>name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="mobile_number">
          <DataField>mobile_number</DataField>
